mixin interiorFinishes()
    .col-lg-12
        .card.shadow.mb-4
            .card-header.py-3.d-flex.flex-row.align-items-center.justify-content-between
                h6.m-0.font-weight-bold.text-primary Interior Finishes
                .dropdown.no-arrow
                    a#dropdownMenuLink.dropdown-toggle(href="#interiorFinishes" data-toggle="collapse" role="button" aria-expanded="true")
                        i.fas.fa-ellipsis-v.fa-sm.fa-fw.text-gray-400
            .card-body.collapse#interiorFinishes
                .row.d-flex.flex-wrap
                    .col.col-12.col-sm-12.col-md-12.col-lg-6
                        h6.m-0.font-weight-bold.text-primary.w-100 Floor Covering
                        div.form-group.form-row
                            .col.col-12.col-sm-12.col-md-8.col-lg-8
                                label.small(for="site-sign") Kitchen
                                .input-group
                                    select.form-control(name="site-sign")
                                        if(project.Interior_finish.kitchen_floor_covering)
                                            each val in forms.option10
                                                if(val.includes(project.Interior_finish.kitchen_floor_covering))
                                                    option(value=val selected) #{val}
                                                else
                                                    option(value=val) #{val}
                                        else
                                            option(value="none" selected disabled hidden) Select an Option
                                            each val in forms.option10 
                                                option(value=val) #{val}
                            .col.col-12.col-sm-12.col-md-4.col-lg-4
                                label.small(for="site-sign") Other
                                .input-group
                                    input.form-control(name="site-sign")
