mixin interiorTrims()
    .col-lg-6
        .card.shadow.mb-4
            .card-header.py-3.d-flex.flex-row.align-items-center.justify-content-between
                h6.m-0.font-weight-bold.text-primary Interior Trims
                .dropdown.no-arrow
                     a#dropdownMenuLink.dropdown-toggle(href="#interior" data-toggle="collapse" role="button" aria-expanded="true")
                         i.fas.fa-ellipsis-v.fa-sm.fa-fw.text-gray-400
            .card-body.collapse#interior
                div.form-group.form-row
                    .col.col-md-12.col-lg-6
                        label.small(for="interiorTrim_scotia_type") Scotia Type
                        .input-group
                            input.form-control(type="text" name="interiorTrim_scotia_type" value=`${project.Interior_trim.scotia_type}`)
                    .col.col-md-12.col-lg-6
                        label.small(for="interiorTrim_skirting_type") Skirting Type
                        .input-group
                            input.form-control(type="text" name="interiorTrim_skirting_type" value=`${project.Interior_trim.skirting_type}`)
                div.form-group.form-row
                    .col.col-md-12.col-lg-6
                        label.small(for="interiorTrim_window_architrave") Window Architrave
                        .input-group
                            input.form-control(type="text" name="interiorTrim_window_architrave" value=`${project.Interior_trim.window_architrave}`)
                    .col.col-md-12.col-lg-6
                        label.small(for="interiorTrim_door_architrave") Door Architrave
                        .input-group
                            input.form-control(type="text" name="interiorTrim_door_architrave" value=`${project.Interior_trim.door_architrave}`)